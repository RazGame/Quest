.container.top-padding-med
  table.table.table-hover.table-striped.table-condensed
    thead
      tr
        th GameApplId
        th Signed Date
        th Game Name
    tbody
      tr(ng-repeat="gameAppl in gameAppls")
        td
          a(href="/game/game-appls/{{gameAppl._id}}") {{gameAppl._id}}
        td {{gameAppl.signed | date}}
        td {{gameAppl.gameParent.title}}